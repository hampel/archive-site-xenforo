{
    "template": "forum_view",
    "description": "Remove log in or register to post from forum view pages",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": "<xf:if is=\"!$forum.canCreateThread() AND !$forum.canCreateThreadPreReg()\">",
    "replace": "<xf:if is=\"!$forum.canCreateThread() AND !$forum.canCreateThreadPreReg() AND false\">"
